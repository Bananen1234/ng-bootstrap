{
  "$schema": "node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ng-bootstrap": {
      "root": "",
      "sourceRoot": "demo-app-projects/ng-bootstrap-demo/src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/ng-bootstrap",
            "index": "demo-app-projects/ng-bootstrap-demo/src/index.html",
            "main": "demo-app-projects/ng-bootstrap-demo/src/main.ts",
            "polyfills": "demo-app-projects/ng-bootstrap-demo/src/polyfills.ts",
            "tsConfig": "demo-app-projects/ng-bootstrap-demo/src/tsconfig.app.json",
            "assets": [
              "demo-app-projects/ng-bootstrap-demo/src/favicon.ico",
              "demo-app-projects/ng-bootstrap-demo/src/assets"
            ],
            "styles": [
              "demo-app-projects/ng-bootstrap-demo/src/styles.scss"
            ],
            "scripts": [
              "node_modules/prismjs/prism.js"
            ],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "demo-app-projects/ng-bootstrap-demo/src/environments/environment.ts",
                  "with": "demo-app-projects/ng-bootstrap-demo/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ng-bootstrap:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ng-bootstrap:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ng-bootstrap:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              "src/styles.scss"
            ],
            "scripts": [],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "demo-app-projects/ng-bootstrap-demo/src/tsconfig.app.json",
              "demo-app-projects/ng-bootstrap-demo/src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "ng-bootstrap-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "ng-bootstrap:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ng-bootstrap:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "bootstrap": {
      "root": "",
      "sourceRoot": "demo-app-projects/bootstrap-demo/src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/bootstrap",
            "index": "demo-app-projects/bootstrap-demo/src/index.html",
            "main": "demo-app-projects/bootstrap-demo/src/main.ts",
            "polyfills": "demo-app-projects/bootstrap-demo/src/polyfills.ts",
            "tsConfig": "demo-app-projects/bootstrap-demo/src/tsconfig.app.json",
            "assets": [
              "demo-app-projects/bootstrap-demo/src/favicon.ico",
              "demo-app-projects/bootstrap-demo/src/assets"
            ],
            "styles": [
              "demo-app-projects/bootstrap-demo/src/styles.scss"
            ],
            "scripts": [],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "demo-app-projects/bootstrap-demo/src/environments/environment.ts",
                  "with": "demo-app-projects/bootstrap-demo/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "bootstrap:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "bootstrap:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "bootstrap:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "demo-app-projects/bootstrap-demo/src/test.ts",
            "polyfills": "demo-app-projects/bootstrap-demo/src/polyfills.ts",
            "tsConfig": "demo-app-projects/bootstrap-demo/src/tsconfig.spec.json",
            "karmaConfig": "demo-app-projects/bootstrap-demo/src/karma.conf.js",
            "styles": [
              "demo-app-projects/bootstrap-demo/src/styles.scss"
            ],
            "scripts": [],
            "assets": [
              "demo-app-projects/bootstrap-demo/src/favicon.ico",
              "demo-app-projects/bootstrap-demo/src/assets"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}